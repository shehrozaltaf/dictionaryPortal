    <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <LinearLayout android:id="@+id/GrpName" style="@style/vlinearlayout">

            <!-- s2t  Title   -->
            <androidx.cardview.widget.CardView
                    android:id="@+id/fldGrpCVs2t"
                    style="@style/cardView">

                <LinearLayout
                        style="@style/vlinearlayout">

                    <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bottom">
                        <TextView
                                android:id="@+id/qtxt_s2t"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@id/q_s2t"
                                android:text="@string/s2t"/>
                        <TextView
                                android:id="@+id/q_s2t"
                                style="@style/quesNum"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignTop="@id/qtxt_s2t"
                                android:layout_alignBottom="@id/qtxt_s2t"
                                android:text="@string/Q_s2t"/>
