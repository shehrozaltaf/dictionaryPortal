                                android:layout_height="wrap_content"
                        />
                        <RadioButton
                                android:id="@+id/db13b"
                                android:text="@string/db13b"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                        />
                    </RadioGroup>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <!-- db14  Input   -->
            <androidx.cardview.widget.CardView
                    android:id="@+id/fldGrpCVdb14"
                    style="@style/cardView"
            >

                <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                >
                    <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bottom">
                    <TextView
                            android:id="@+id/qtxt_db14"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/q_db14"
                            android:text="@string/db14"/>
                        <TextView
                                android:id="@+id/q_db14"
                                style="@style/quesNum"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignTop="@id/qtxt_db14"
                                android:layout_alignBottom="@id/qtxt_db14"
                                android:text="@string/Q_db14"/>
                    </RelativeLayout>
                    <EditText
                            android:id="@+id/db14"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="@string/db14"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <!-- db15  Input-Numeric   -->
            <androidx.cardview.widget.CardView
                    android:id="@+id/fldGrpCVdb15"
                    style="@style/cardView"
            >

                <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                >
                    <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bottom">
                    <TextView
                            android:id="@+id/qtxt_db15"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/q_db15"
                            android:text="@string/db15"/>
                        <TextView
                                android:id="@+id/q_db15"
                                style="@style/quesNum"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignTop="@id/qtxt_db15"
                                android:layout_alignBottom="@id/qtxt_db15"
                                android:text="@string/Q_db15"/>
                    </RelativeLayout>
                    <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/db15"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="@string/db15"
                            android:inputType="number"
                            android:maxLength="2"
                            app:maxValue="10"
                            app:minValue="1"
                            app:type="range"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <!-- db16  Radio   -->
            <androidx.cardview.widget.CardView
                    android:id="@+id/fldGrpCVdb16"
                    style="@style/cardView"
            >

                <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                >
                    <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bottom">
                    <TextView
                            android:id="@+id/qtxt_db16"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/q_db16"
                            android:text="@string/db16"/>
                    <TextView
                            android:id="@+id/q_db16"
                            style="@style/quesNum"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignTop="@id/qtxt_db16"
                            android:layout_alignBottom="@id/qtxt_db16"
                            android:text="@string/Q_db16"/>
                    </RelativeLayout>
                    <RadioGroup
                            android:id="@+id/db16"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                        <RadioButton
                                android:id="@+id/db16a"
                                android:text="@string/db16a"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                        />
                        <RadioButton
                                android:id="@+id/db1696"
                                android:text="@string/db1696"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>
                        <EditText
                                android:id="@+id/db1696x"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="@string/db1696"
                                android:digits="AaBaCcDdEeFfGgHhIiJjKkLlMmNnOoPpQqRrSsTtUuVvWwXxYyZz0123456789,. "
                                android:tag="db1696"
                                android:text='@{db1696.checked ? db1696x.getText().toString() : ""}'
                                android:visibility='@{db1696.checked? View.VISIBLE : View.GONE}'/>
                    </RadioGroup>
                </LinearLayout>
            </androidx.cardview.widget.CardView><!--EndButton LinearLayout-->
            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="end"
                    android:layout_marginTop="24dp"
                    android:orientation="horizontal">
                <Button
                        android:id="@+id/btn_End"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp"
                        android:onClick="@{() -> callback.BtnEnd()}"
                        android:background="@color/red_overlay"
                        android:textColor="@color/white"
                        android:text="Cancel"/>
                <Button
                        android:id="@+id/btn_Continue"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp"
                        android:onClick="@{() -> callback.BtnContinue()}"
                        android:background="@color/green_overlay"
                        android:textColor="@color/white"
                        android:text="Save"/>
            </LinearLayout></LinearLayout>
    </ScrollView>
</layout>