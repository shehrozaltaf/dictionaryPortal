                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <!-- muac1  Input-Numeric   -->
<androidx.cardview.widget.CardView
        android:id="@+id/fldGrpCVmuac1"
        style="@style/cardView">

    <LinearLayout
            style="@style/vlinearlayout">

        <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bottom">
            <TextView
                    android:id="@+id/qtxt_muac1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@id/q_muac1"
                    android:text="@string/muac1"/>
            <TextView
                    android:id="@+id/q_muac1"
                    style="@style/quesNum"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@id/qtxt_muac1"
                    android:layout_alignBottom="@id/qtxt_muac1"
                    android:text="@string/Q_muac1"/>
        </RelativeLayout>


        <com.edittextpicker.aliazaz.EditTextPicker
                android:id="@+id/muac1"
                style="@style/EditTextNum"
                android:hint="@string/muac1"
                app:mask="##"
                app:maxValue="21"
                app:minValue="11"
                app:type="range"/>
    </LinearLayout>
</androidx.cardview.widget.CardView>

